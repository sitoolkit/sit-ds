{
  "name": "ldap",
  "protocol": "LDAP",
  "useTrustStore": false,
  "host": "{{ .Env.LDAP_HOST }}",
  "port": "{{ .Env.LDAP_PORT }}",
  "searchBase": "{{ .Env.LDAP_ROOT_DN }}",
  "authScheme": "SIMPLE",
  "authRealm": null,
  "authUsername": "{{ .Env.LDAP_MANAGER_DN }}",
  "connectionTimeoutSeconds": 30,
  "connectionRetryDelaySeconds": 300,
  "maxIncidentsCount": 3,
  "userBaseDn": "",
  "userSubtree": true,
  "userObjectClass": "{{ .Env.LDAP_USER_OBJECTCLASS }}",
  "userLdapFilter": "",
  "userIdAttribute": "{{ .Env.LDAP_LOGIN_ATTRIBUTE }}",
  "userRealNameAttribute": "{{ .Env.LDAP_FULLNAME_ATTRIBUTE }}",
  "userEmailAddressAttribute": "{{ .Env.LDAP_MAIL_ATTRIBUTE }}",
  "userPasswordAttribute": "",
  "ldapGroupsAsRoles": true,
  "groupType": "DYNAMIC",
  "groupBaseDn": null,
  "groupSubtree": false,
  "groupObjectClass": null,
  "groupIdAttribute": null,
  "groupMemberAttribute": null,
  "groupMemberFormat": null,
  "userMemberOfAttribute": "objectClass",
  "authPassword": "{{ .Env.LDAP_MANAGER_PASSWORD }}"
}
